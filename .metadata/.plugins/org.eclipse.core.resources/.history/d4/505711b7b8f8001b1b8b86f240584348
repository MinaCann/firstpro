package loop01;

public class Fibonacci {

	public static void main(String[] args) {
		//Fibonacci
		//1 1 2 3 5 8 13 21
		
		int a=1;
		int b=1;
		int c;
		System.out.print(a+" ");
		System.out.print(b+" ");
		for(int i=0;i<10; i++) {
			c=a+b;
			a=b;
			b=c;
			System.out.print(c+" " );
		}
	}

}
