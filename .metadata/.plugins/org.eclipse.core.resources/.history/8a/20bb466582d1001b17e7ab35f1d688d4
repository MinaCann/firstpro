package day10loops;

public class LoopReview {

	public static void main(String[] args) {
		
		for(int i=33 ; i<201; i=i+2) {
			System.out.print(i +" ");
		}
		System.out.println();
		for(int i=200; i>32; i=i-1)	{
			if(i%2 != 0) {
				
			
			System.out.print(i + " ");
			}
			
			for(int k= 23 ; k<58 ; k++) {
			int k = k+ (k+1);
				System.out.print(k); 
			}
		}
		}
	}


