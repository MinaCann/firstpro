package day11Loop01;

public class ForLoop02 {

	public static void main(String[] args) {
		/*
		 Type code to find the multiplication of the numbers from 1 to 10 
		 */
		
		int prod = 1;
		
		for(int i=-2; i<5; i++) {
			prod= prod * i;
			
		} 
		System.out.println(prod);
		
		
		
		
		
		
		

	}

}
